{
  "inbounds": [
    {
      "type": "vless",
      "tag": "in-vless",
      "listen": "127.0.0.1",
      "port": 12345,
      "sniff": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      },
      "users": [
        {
          "id": "de3eca5d-7c3a-4a57-aede-51f5ab1c82b9",
          "flow": "xtls-rprx-vision"
        }
      ],
      "transport": {
        "network": "ws",
        "wsSettings": {
          "path": "/vless"
        }
      }
    }
  ],
  "outbounds": [
    {
      "type": "freedom",
      "tag": "out-freedom"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "inboundTag": ["in-vless"],
        "outboundTag": "out-freedom"
      }
    ]
  }
}
